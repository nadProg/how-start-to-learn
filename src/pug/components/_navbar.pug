-
  let links = [
    {
      anchor: 'books',
      text: 'Книги'
    },
    {
      anchor: 'online-books',
      text: 'Он-лайн учебники'
    },
    {
      anchor: 'interactives',
      text: 'Тренажеры'
    },
    {
      anchor: 'video',
      text: 'Видео-уроки'
    },
    {
      anchor: 'other',
      text: 'Разное'
    },
  ]

.navbar-fixed
  nav.green.lighten-2
    a(href="#" data-target="mobile-menu" class="sidenav-trigger"): i.material-icons menu
    ul.hide-on-med-and-down(style="display: flex; justify-content: center")
      each link in links
        li: a(href=`#${link.anchor}`)= link.text
      li: a(href='#') Обо мне

ul#mobile-menu.sidenav.sidenav-close.green.lighten-4
  each link in links
    li: a.grey-blue-text(href=`#${link.anchor}`)= link.text
  li: .divider.green
  li: a.grey-blue-text(href='#') Обо мне
